<?xml version='1.0' encoding='utf-8'?>
<litp:volume-group xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:litp="http://www.ericsson.com/litp" xsi:schemaLocation="http://www.ericsson.com/litp litp-xml-schema/litp.xsd" id="vg1">
  <volume_group_name>vg_root<!--note: this property is not updatable--></volume_group_name>
  <litp:volume-group-file_systems-collection id="file_systems">
    <litp:file-system id="root">
      <backup_policy>live</backup_policy>
      <mount_point>/<!--note: this property is not updatable--></mount_point>
      <size>9G</size>
      <snap_external>false</snap_external>
      <snap_size>100</snap_size>
      <type>ext4<!--note: this property is not updatable--></type>
    </litp:file-system>
    <litp:file-system id="swap">
      <mount_point>swap<!--note: this property is not updatable--></mount_point>
      <size>2G</size>
      <snap_external>false</snap_external>
      <snap_size>100</snap_size>
      <type>swap<!--note: this property is not updatable--></type>
    </litp:file-system>
  </litp:volume-group-file_systems-collection>
  <litp:volume-group-physical_devices-collection id="physical_devices">
    <litp:physical-device id="additional1">
      <device_name>hd5<!--note: this property is not updatable--></device_name>
    </litp:physical-device>
    <litp:physical-device id="internal">
      <device_name>hd0<!--note: this property is not updatable--></device_name>
    </litp:physical-device>
    <litp:physical-device id="fun03_lvm_1">
    <device_name>fun03_lvm_1</device_name>
  </litp:physical-device>
  </litp:volume-group-physical_devices-collection>
</litp:volume-group>
